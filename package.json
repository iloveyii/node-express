{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build-server": "npm run build-ts && npm run tslint && npm run cp-env",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "cp-env": "cp .env-production dist/.env -f",
    "serve-debug": "nodemon --inspect dist/server/server.js",
    "build": "npm run build-server && concurrently \"npm run serve\" ",
    "start": "ts-node server.ts",
    "dev": "nodemon server.ts",
    "prestart": "npx sequelize db:create --config=./sequelize/src/config/config.js",
    "serve": "cd dist && node server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "npx sequelize db:create --config=./sequelize/src/config/config.js && npm run db:reset",
    "db:reset": "ts-node src/models/Mongo_reset.ts && cd sequelize && npm run test"
  },
  "keywords": [
    "node",
    "ts",
    "tslint",
    "yarn"
  ],
  "author": "Ali",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/dotenv": "^8.2.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.5.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "mysql2": "^2.1.0",
    "node-tsc": "0.0.14",
    "sequelize": "^5.21.13",
    "ts-node": "^8.10.2",
    "tsc": "^1.20150623.0",
    "tsc-node": "0.0.3",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.17.6",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.13",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.2",
    "jest": "^26.1.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "typescript": "^3.9.5"
  },
  "_moduleAliases": {
    "@sequelize": "sequelize/src"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
